        var imagesOffline = document.getElementsByClassName('offline-image');\r\n          function scriptLoad (scripts, num) {\r\n              if (num < scripts.length) {\r\n                  document.getElementsByTagName('head')[0].appendChild(scriptsToAppend[num]);\r\n                  scriptsToAppend[num].onload = function () {\r\n                      console.log('script %s loaded', scriptsToAppend[num]);\r\n                      scriptLoad(scripts, ++num);\r\n                  };\r\n                  scriptsToAppend[num].onerror = function () {\r\n                      console.log('script %s could not be loaded', scriptsToAppend[num]);\r\n                      scriptLoad(scripts, ++num);\r\n                  };\r\n                  scriptsToAppend[num].setAttribute('src', externalScriptsSrc[num]);\r\n              } else {\r\n                  var event = new Event('scriptsLoaded');\r\n                  document.dispatchEvent(event);\r\n              }\r\n          }\r\n          for(var i = 0; i < styleSheets.length; i++) {\r\n              if (styleSheets[i].getAttribute('data-href')) {\r\n                  if (projectIsPreview) {\r\n                      styleSheets[i].setAttribute('href', styleSheets[i].getAttribute('data-href'));\r\n                  } else {\r\n                      styleSheets[i].setAttribute('href', styleSheets[i].getAttribute('data-href').replace(\/^(http)(.*)(uploads\\\/)\/gi, ''));\r\n                  }\r\n              }\r\n          }\r\n\r\n          for (var i = 0; i < fontStyles.length; i++) {\r\n              if (fontStyles[i].getAttribute('data-font-src')) {\r\n                  if (projectIsPreview) {\r\n                      fontStyles[i].innerHTML = '@font-face {font-family: \"' + fontStyles[i].getAttribute('data-font-family') + '\"; src: url(\"' + fontStyles[i].getAttribute('data-font-src') + '\") format(\"truetype\");}';\r\n                  } else {\r\n                      fontStyles[i].innerHTML = '@font-face {font-family: \"' + fontStyles[i].getAttribute('data-font-family') + '\"; src: url(\"' + fontStyles[i].getAttribute('data-font-src').replace(\/^(http)(.*)(uploads\\\/)\/gi, '') + '\") format(\"truetype\");}';\r\n                  }\r\n              }\r\n          }\r\n\r\n          for (var i = 0; i < scriptsLen; i++) {\r\n              if (scripts[i].getAttribute('data-src')) {\r\n                  if (projectIsPreview) {\r\n                      externalScriptsSrc.push(scripts[i].getAttribute('data-src'));\r\n                  } else {\r\n                      externalScriptsSrc.push(scripts[i].getAttribute('data-src').replace(\/^(http)(.*)(uploads\\\/)\/gi, ''));\r\n                  }\r\n                  scriptsToAppend.push(document.createElement('script'));\r\n              }\r\n          }\r\n\r\n          for (var i = 0; i < imagesOffline.length; i++) {\r\n              if (imagesOffline[i].getAttribute('data-src')) {\r\n                  if (projectIsPreview) {\r\n                      imagesOffline[i].style.backgroundImage = 'url(' + imagesOffline[i].getAttribute('data-src') + ')';\r\n                  } else {\r\n                      imagesOffline[i].style.backgroundImage = 'url(' + imagesOffline[i].getAttribute('data-src').replace(\/^(http)(.*)(uploads\\\/)\/gi, '') + ')';\r\n                  }\r\n              }\r\n          }\r\n          scriptLoad(externalScriptsSrc, 0);\r\n      })();\r\n  <\/script>\r\n  <script>\r\n      document.addEventListener('scriptsLoaded', function () {\r\n          console.log('scriptsLoadedEvent');\r\n\r\n          \/* fast click init *\/\r\n          FastClick.attach(document.body);\r\n\r\n          \/* init mvc classes *\/\r\n          var model = new QwizBuildrModel();\r\n          var view = new QwizBuildrView(model);\r\n          var controller = new QwizBuildrController(model, view);\r\n          \/* init *\/\r\n          controller.init();\r\n\r\n          var $body = $('body');\r\n          (function () {\r\n              $(\"#close-quiz\").on(\"click\", function () {\r\n                  if (window.webkit && window.webkit.messageHandlers && window.webkit.messageHandlers && window.webkit.messageHandlers.GK) {\r\n                      window.webkit.messageHandlers.GK.postMessage({close: \"quiz\"});\r\n                  }\r\n              });\r\n              var orientations = {\r\n                  '90': 'landscape',\r\n                  '-90': 'landscape',\r\n                  '0': 'portrait',\r\n                  '180': 'portrait'\r\n              };\r\n\r\n              function calculateSize () {\r\n                  var size = 1;\r\n                  if (projectIsPreview) {\r\n                      \/* this code is to trigger sizes in the preview *\/\r\n                      size = 3;\r\n                      if ($body.width() > 746) {\r\n                          size = 5;\r\n                      }\r\n                      return size;\r\n                  }\r\n\r\n                  if (window.innerHeight > window.innerWidth) {\r\n                      if (window.innerHeight > 480) {\r\n                          size = 2;\r\n                      }\r\n                      if (window.innerHeight > 568) {\r\n                          size = 3;\r\n                      }\r\n                      if (window.innerHeight > 667) {\r\n                          size = 4;\r\n                      }\r\n                      if (window.innerHeight > 736) {\r\n                          size = 5;\r\n                      }\r\n                      if (window.innerHeight > 1024) {\r\n                          size = 6;\r\n                      }\r\n                  } else {\r\n                      if (window.innerWidth > 480) {\r\n                          size = 2;\r\n                      }\r\n                      if (window.innerWidth > 568) {\r\n                          size = 3;\r\n                      }\r\n                      if (window.innerWidth > 667) {\r\n                          size = 4;\r\n                      }\r\n                      if (window.innerWidth > 736) {\r\n                          size = 5;\r\n                      }\r\n                      if (window.innerWidth > 1024) {\r\n                          size = 6;\r\n                      }\r\n                  }\r\n                  return size;\r\n              }\r\n\r\n              $body.addClass('size-' + calculateSize()).addClass(typeof orientation !== \"undefined\" ? orientations[orientation] : 'landscape');\r\n\r\n              $(window).on('resize', function () {\r\n                  $body.removeClass('size-1 size-2 size-3 size-4 size-5 size-6').addClass('size-' + calculateSize());\r\n                  console.log(calculateSize());\r\n              });\r\n\r\n              $(window).on('orientationchange', function () {\r\n                  $body.removeClass('landscape portrait').addClass(typeof orientation !== \"undefined\" ? orientations[orientation] : 'landscape');\r\n              });\r\n          })();\r\n      });\r\n  <\/script>   \r\n<\/body>\r\n<\/html>\r\n"
